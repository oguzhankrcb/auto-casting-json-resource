{
    "name": "oguzhankrcb/auto-casting-json-resource",
    "description": "This Laravel package automatically casts your JsonResource data using the casting functions you have defined before.",
    "keywords": [
        "oguzhankrcb",
        "laravel",
        "auto-casting-json-resource"
    ],
    "homepage": "https://github.com/oguzhankrcb/auto-casting-json-resource",
    "license": "MIT",
    "authors": [
        {
            "name": "OÄŸuzhan KARACABAY",
            "email": "oguzhankrcb@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0 || ^8.1",
        "spatie/laravel-package-tools": "^1.9.2",
        "laravel/framework": "^8.0 || ^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Oguzhankrcb\\AutoCastingJsonResource\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oguzhankrcb\\AutoCastingJsonResource\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Oguzhankrcb\\AutoCastingJsonResource\\AutoCastingJsonResourceServiceProvider"
            ],
            "aliases": {
                "AutoCastingJsonResource": "Oguzhankrcb\\AutoCastingJsonResource\\Facades\\AutoCastingJsonResource"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
